<Page xmlns="http://www.nativescript.org/tns.xsd"
      xmlns:g="components/grid-view/grid-view"
      loaded="pageLoaded"
      navigatedTo="navigatedTo" >
  <Page.actionBar>
    <ActionBar>
      <ActionBar.actionItems>
        <ActionItem text="DONE" tap="doneMenuItemTap" android.position="actionBar" />
      </ActionBar.actionItems>
    </ActionBar>
  </Page.actionBar>
  <GridLayout>
    <TextField text="{{ item.Title }}" />
  </GridLayout>
</Page>