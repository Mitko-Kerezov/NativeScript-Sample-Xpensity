<Page xmlns="http://www.nativescript.org/tns.xsd"
      loaded="pageLoaded"
      navigatedTo="navigatedTo" >
  <ListView items="{{ items }}" itemTap="itemTap" >
    <ListView.itemTemplate>
      <Label text="{{ data }}" cssClass="{{ isSelected ? 'item-selected' : 'item' }}" />
    </ListView.itemTemplate>
  </ListView>
</Page>